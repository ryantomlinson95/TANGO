<% if user_signed_in? %>
<% end %>

<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Github URL</th>
      <th>Progress</th>
      <th>Owner</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.description %></td>
        <td><%= project.githuburl %></td>
        <td><%= project.progress %></td>
        <td><%= project.owner %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>
